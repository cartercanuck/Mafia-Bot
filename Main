public class Main {

	public static String Token = "NzcxOTE3NTgzMzYzNDczNDM5.X5zGRQ.mM91bRepVLhwoV7N3oTx3KAhqow";
	public static void main(String[] args) throws LoginException, InterruptedException {
		
		@SuppressWarnings("unused")
		JDA builder = JDABuilder.createDefault(Token).setActivity(Activity.playing("Mafia"))
